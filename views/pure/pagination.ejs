<div>
    <ul class="pure-paginator">
        <% if(page-1){ %>
        <li>
            <button class="pure-button prev" style="height: 3em;" onclick="location.href='/?p=<%= page-1 %>'"><%= __('PREVIOUS') %></button>
        </li>
        <% } %>
        <% if(postsLen>=10){ %>
        <li>
            <button class="pure-button next" style="height: 3em;" onclick="location.href='/?p=<%= page+1 %>'"><%= __('NEXT') %></button>
        </li>
        <% } %>
    </ul>
</div>