<div>
    <ul class="pure-paginator">
        <% if(page-1){ %>
        <li>
            <!-- <button class="pure-button prev" style="height: 3em;"> </button> -->
            <a href="/?p=<%= page-1 %>" class="pure-button prev"><%= __('PREVIOUS') %></a>
        </li>
        <% } %>
        <% if(postsLen>=10){ %>
        <li>
            <!-- <button class="pure-button next" style="height: 3em;"></button> -->
            <a href="/?p=<%= page+1 %>" class="pure-button next"><%= __('NEXT') %></a>
        </li>
        <% } %>
    </ul>
</div>